<?xml version="1.0" encoding="UTF-8" ?>
<!-- Backtracking patterns specify the class, method and arguments which are considered as SINKS,
	and will be backtracked -->
<backtracking-patterns xmlns="http://syssec.rub.de/SAAF"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://syssec.rub.de/SAAF schema/backtracking-patterns.xsd">

	<!-- TCP SOCKETS -->

	<!-- java.net.Socket constructors -->

	<backtracking-pattern active="true"
		class="java/net/Socket"
		description="TCP socket constructor"
		method="&lt;init&gt;"
		parameters="Ljava/net/InetAddress;I"
		interesting="0" />

	<backtracking-pattern active="true"
		class="java/net/Socket"
		description="TCP socket constructor"
		method="&lt;init&gt;"
		parameters="Ljava/net/InetAddress;ILjava/net/InetAddress;I"
		interesting="0" />
	
	<backtracking-pattern active="true"
		class="java/net/Socket"
		description="TCP socket constructor"
		method="&lt;init&gt;"
		parameters="Ljava/lang/String;I"
		interesting="0" />

	<backtracking-pattern active="true"
		class="java/net/Socket"
		description="TCP socket constructor"
		method="&lt;init&gt;"
		parameters="Ljava/lang/String;IZ"
		interesting="0" />
	
	<backtracking-pattern active="true"
		class="java/net/Socket"
		description="TCP socket constructor"
		method="&lt;init&gt;"
		parameters="Ljava/lang/String;ILjava/net/InetAddress;I"
		interesting="0" />
	
	<backtracking-pattern active="true"
		class="java/net/Socket"
		description="TCP socket constructor"
		method="&lt;init&gt;"
		parameters="Ljava/net/SocketAddress;Ljava/net/SocketAddress;Z"
		interesting="0" />

	
	<!-- java.net.Socket.bind -->
	<backtracking-pattern active="true"
		class="java/net/Socket"
		description="TCP socket"
		method="bind"
		parameters="Ljava/net/SocketAddress;"
		interesting="0" />
	

	<!-- java.net.Socket.connect -->

	<backtracking-pattern active="true"
		class="java/net/Socket"
		description="TCP socket connect"
		method="connect"
		parameters="Ljava/net/SocketAddress;"
		interesting="0" />

	<backtracking-pattern active="true"
		class="java/net/Socket"
		description="TCP socket connect"
		method="connect"
		parameters="Ljava/net/SocketAddress;I"
		interesting="0" />

	
	<!-- UDP SOCKETS -->

	<!-- java.net.DatagramSocket constructors -->

	<backtracking-pattern active="true"
		class="java/net/DatagramSocket"
		description="UDP socket constructor"
		method="&lt;init&gt;"
		parameters=""
		interesting="0" />
	
	<backtracking-pattern active="true"
		class="java/net/DatagramSocket"
		description="UDP socket constructor"
		method="&lt;init&gt;"
		parameters="I"
		interesting="0" />

	<backtracking-pattern active="true"
		class="java/net/DatagramSocket"
		description="UDP socket constructor"
		method="&lt;init&gt;"
		parameters="Ljava/net/SocketAddress;"
		interesting="0" />

	<backtracking-pattern active="true"
		class="java/net/DatagramSocket"
		description="UDP socket constructor"
		method="&lt;init&gt;"
		parameters="ILjava/net/InetAddress;"
		interesting="0" />

	
	<!-- java.net.DatagramSocket.bind -->
	<backtracking-pattern active="true"
		class="java/net/DatagramSocket"
		description="UDP socket bind"
		method="bind"
		parameters="Ljava/net/SocketAddress;"
		interesting="0" />

	
	<!-- java.net.DatagramSocket.connect -->

	<backtracking-pattern active="true"
		class="java/net/DatagramSocket"
		description="UDP socket connect"
		method="connect"
		parameters="Ljava/net/SocketAddress;"
		interesting="0" />

	<backtracking-pattern active="true"
		class="java/net/DatagramSocket"
		description="UDP socket connect"
		method="connect"
		parameters="Ljava/net/InetAddress;I"
		interesting="0" />


	<!-- java.net.DatagramSocket.send -->
	<backtracking-pattern active="true"
		class="java/net/DatagramSocket"
		description="UDP socket send"
		method="send"
		parameters="Ljava/net/DatagramPacket;"
		interesting="0" />

	<!-- java.net.DatagramSocket.receive -->
	<backtracking-pattern active="true"
		class="java/net/DatagramSocket"
		description="UDP socket receive"
		method="receive"
		parameters="Ljava/net/DatagramPacket;"
		interesting="0" />



	<backtracking-pattern active="true"
		class="javax/net/ssl/SSLSocket" description="SSL Cipher Suites"
		method="setEnabledCipherSuites" parameters="[Ljava/lang/String;"
		interesting="0" />

	<backtracking-pattern active="true"
		class="javax/net/ssl/SSLSocketFactory" description="SSL Connections"
		method="createSocket" parameters="Ljava/lang/String;I" interesting="0" />

	<backtracking-pattern active="true"
		class="javax/net/ssl/SSLSocketFactory" description="SSL Connections"
		method="createSocket" parameters="Ljava/lang/String;I" interesting="1" />

